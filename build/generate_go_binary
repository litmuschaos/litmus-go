# Buiding go binaries for container_kill helper(containerd,crio)
go build -o build/_output/${GOARCH}/helper/container-killer ./chaoslib/litmus/container-kill/helper/containerd
# Buiding go binaries for container_kill helper(docker)
go build -o build/_output/${GOARCH}/helper/container-killer-docker ./chaoslib/litmus/container-kill/helper/docker
# Buiding go binaries for network_chaos helper
go build -o build/_output/${GOARCH}/helper/network-chaos ./chaoslib/litmus/network-chaos/helper
# Building go binaries for all experiments
go build -o build/_output/${GOARCH}/experiments ./bin
