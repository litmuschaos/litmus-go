# Buiding go binaries for container_kill helper
go build -o build/_output/${GOARCH}/helper/container-killer ./chaoslib/litmus/container-kill/helper -mod=vendor
# Buiding go binaries for network_chaos helper
go build -o build/_output/${GOARCH}/helper/network-chaos ./chaoslib/litmus/network-chaos/helper -mod=vendor
# Building go binaries for all experiments
go build -o build/_output/${GOARCH}/experiments ./bin -mod=vendor
