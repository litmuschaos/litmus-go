# Buiding go binaries for container_kill helper
go build -o build/_output/${GOARCH}/helper/container-killer ./chaoslib/litmus/container-kill/helper
# Buiding go binaries for network_chaos helper(containerd,crio)
go build -o build/_output/${GOARCH}/helper/network-chaos ./chaoslib/litmus/network-chaos/helper/containerd
# Buiding go binaries for network_chaos helper(docker)
go build -o build/_output/${GOARCH}/helper/network-chaos-docker ./chaoslib/litmus/network-chaos/helper/docker
# Building go binaries for all experiments
go build -o build/_output/${GOARCH}/experiments ./bin
